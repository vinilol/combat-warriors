_G.key = "put key here IDIOT" -- user should put his key here.
local whitelistecheck = loadstring(game:HttpGet("https://raw.githubusercontent.com/vinilol/vini-things/main/the%20real%20script%20here%3A", true))() -- pastebin link there
if whitelistecheck[_G.key] then
loadstring(game:HttpGet("https://raw.githubusercontent.com/vinilol/vini-things/main/the%20real%20script%20here%3A")()
else
game:service('Players').LocalPlayer:Kick('Not Whitelisted')
end

